# Staging Environment Configuration
# Copy this to .env and update values for your staging environment

# Application
APP_NAME="Second Brain Staging"
APP_ENV=staging
LOG_LEVEL=info
APP_PORT=8000

# Database
DB_USER=secondbrain
DB_PASSWORD=staging_password_change_me
DB_NAME=secondbrain_staging

# Redis
REDIS_PASSWORD=staging_redis_change_me

# Security
SECRET_KEY=your_staging_secret_key_here_change_me
CORS_ORIGINS=["http://localhost:3000","http://staging.yourdomain.com"]

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Nginx Ports
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# Monitoring
GRAFANA_USER=admin
GRAFANA_PASSWORD=staging_grafana_change_me
GRAFANA_PORT=3001

# Admin Tools
ADMINER_PORT=8081

# Feature Flags
USE_MOCK_DATABASE=false
ENABLE_TELEMETRY=true
ENABLE_ANALYTICS=true