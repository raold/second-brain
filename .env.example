# Environment Type: production, ci, test
APP_ENV=production

# OpenAI Settings
OPENAI_API_KEY=your-openai-api-key
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# API Tokens (comma-separated)
API_TOKENS=your-token-1,your-token-2

# Qdrant Settings
QDRANT_HOST=qdrant
QDRANT_PORT=6333
QDRANT_COLLECTION=second_brain
QDRANT_VECTOR_SIZE=1536
QDRANT_DISTANCE=Cosine

# Logging
LOG_PATH=logs/processor.log
LOG_LEVEL=INFO

# OpenAI Retry Settings
OPENAI_RETRY_ATTEMPTS=3
OPENAI_RETRY_MULTIPLIER=1
OPENAI_RETRY_MIN_WAIT=2
OPENAI_RETRY_MAX_WAIT=10